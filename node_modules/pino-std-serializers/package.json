    "test",
    "test-types"
  ],
  "devDependencies": {
    "@matteo.collina/tspl": "^0.1.1",
    "@types/node": "^20.11.17",
    "borp": "^0.9.1",
    "pre-commit": "^1.2.2",
    "snazzy": "^9.0.0",
    "standard": "^17.1.0",
    "tsd": "^0.31.0",
